{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "strict": true,
    "skipLibCheck": true,
    "types": ["node"],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "outDir": "./dist",
    "plugins": [
      {
        "name": "@effect/language-service"
      }
    ]
  },
  "exclude": ["node_modules"]
}
